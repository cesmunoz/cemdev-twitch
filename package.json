{
  "name": "cembot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "turbo run typecheck",
    "dev": "turbo run dev",
    "dev:client": "turbo run dev --scope=client",
    "dev:server": "turbo run dev --scope=server --include-dependencies --no-deps --parallel",
    "build": "turbo run build",
    "build:client": "turbo run build --scope=client",
    "build:server": "turbo run build --scope=server",
    "format": "turbo run format",
    "format:client": "turbo run format --scope=client",
    "format:server": "turbo run format --scope=server",
    "lint": "turbo run lint",
    "lint:client": "turbo run lint --scope=client",
    "lint:server": "turbo run lint --scope=server",
    "lint:fix": "turbo run lint:fix",
    "lint:fix:client": "turbo run lint:fix --scope=client",
    "lint:fix:server": "turbo run lint:fix --scope=server",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesmunoz/cembot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cesmunoz/cembot/issues"
  },
  "homepage": "https://github.com/cesmunoz/cembot#readme",
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "turbo": "^1.1.2"
  },
  "packageManager": "pnpm@7.15.0",
  "volta": {
    "node": "18.12.0"
  }
}
