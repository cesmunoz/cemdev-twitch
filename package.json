  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "turbo": "^1.1.2"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  }
