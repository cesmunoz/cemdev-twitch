{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "typecheck": { "outputs": [] },
    "format": { "outputs": [] },
    "lint": { "outputs": [] },
    "lint:fix": { "outputs": [] },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "dev:ntl": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
